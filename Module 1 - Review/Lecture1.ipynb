    "from scipy.special import comb\n",
    "\n",
    "# Problem parameters\n",
    "k = 63\n",
    "n = 31000\n",
    "p = 0.00203\n",
    "\n",
    "result = comb(n, k) * (p ** k) * ((1 - p) ** (n - k))\n",
    "print(result)"
